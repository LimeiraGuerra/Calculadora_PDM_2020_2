<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calculadora_pdm_2020_2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hello_world_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hello_world_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.CalculadoraPDM20202">
        <activity android:name=".ConfiguracoesActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>